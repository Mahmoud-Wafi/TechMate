{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "env": {
        "VITE_API_URL": "@vite_api_url"
    },
    "routes": [{
            "src": "/api/(.*)",
            "dest": "https://your-railway-backend.railway.app/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}